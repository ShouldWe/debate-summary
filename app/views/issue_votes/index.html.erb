<h1>Listing issue_votes</h1>

<table>
  <tr>
    <th>Issue vote</th>
    <th>User</th>
    <th>Ip address</th>
    <th>Up or down</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @issue_votes.each do |issue_vote| %>
  <tr>
    <td><%= issue_vote.issue_vote_id %></td>
    <td><%= issue_vote.user_id %></td>
    <td><%= issue_vote.ip_address %></td>
    <td><%= issue_vote.up_or_down %></td>
    <td><%= link_to 'Show', issue_vote %></td>
    <td><%= link_to 'Edit', edit_issue_vote_path(issue_vote) %></td>
    <td><%= link_to 'Destroy', issue_vote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Issue vote', new_issue_vote_path %>
