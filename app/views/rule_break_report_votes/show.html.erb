<%- model_class = RuleBreakReportVote -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:user_id) %>:</strong></dt>
  <dd><%= @rule_break_report_vote.user_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:penalty_id) %>:</strong></dt>
  <dd><%= @rule_break_report_vote.penalty_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:rule_break_report_id) %>:</strong></dt>
  <dd><%= @rule_break_report_vote.rule_break_report_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:apply_unilaterally) %>:</strong></dt>
  <dd><%= @rule_break_report_vote.apply_unilaterally %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              rule_break_report_votes_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_rule_break_report_vote_path(@rule_break_report_vote), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              rule_break_report_vote_path(@rule_break_report_vote),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
